<launch>
    <!-- <arg name="mav_name" default="firefly" />
    <arg name="namespace" default="$(arg mav_name)"/> -->
    <!-- <group ns="$(arg namespace)"> -->
    <group ns="magnus">
        <node pkg="imav_trajectory_generator" type="magnus_trajectory_node" name="magnus_trajectory_node" clear_params="true" output="screen"/>
        <node pkg="imav_trajectory_generator" type="trajectory_sampler_node" name="trajectory_sampler_node" clear_params="true" output="screen"/>
    </group>
    <group ns="vega">
        <node pkg="imav_trajectory_generator" type="vega_trajectory_node" name="vega_trajectory_node" clear_params="true" output="screen"/>
        <node pkg="imav_trajectory_generator" type="trajectory_sampler_node" name="trajectory_sampler_node" clear_params="true" output="screen"/>
    </group>
    <group ns="icarus">
        <node pkg="imav_trajectory_generator" type="icarus_trajectory_node" name="icarus_trajectory_node" clear_params="true" output="screen"/>
        <node pkg="imav_trajectory_generator" type="trajectory_sampler_node" name="trajectory_sampler_node" clear_params="true" output="screen"/>
    </group>
    <!-- </group> -->
</launch>